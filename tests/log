##foobar #echo "foobar"## hello ##world #echo "$hello $world"#{"hello":"hello","world":"world"}# hello ##moon #echo "$hello $moon"#{"hello":"hello","moon":"moon"}# cat #makes #meow #cat##meow cat #makes #meeoow #cat##meeoow eilsel ##tsief #sort $SORT_OPTION#{"SORT_OPTION":"-r"}#3 1 2 unix ##timestamp #echo -n "$(cat -)" && echo -n ' 0 is ' && echo "$DATE"#{"DATE":"$(date -d @0 +\"%a %d %b %Y\")"}#unix timestamp duplicate ##entry #echo 2## space in group #space in subgroup #space in command #echo 'whoa'## ##bar #echo 'first bar'## foo ##bar #echo 'second bar'## ##space #echo $VARIABLE#{"VARIABLE":"hello world"}# ##env starting with digit #echo $VARIABLE#{"1ENV":"hello world","VARIABLE":"hello world"}# ##env containing space #echo $VARIABLE#{"VAR IABLE":"hello world","VARIABLE":"hello world"}# ##env is empty #echo $VARIABLE#{"":"hello world","VARIABLE":"hello world"}# ##env containing hyphen #echo $VARIABLE#{"VAR-IABLE":"hello world","VARIABLE":"hello world"}# group #subgroup #foo #echo "foo"## group ##bar #echo "bar"## ##pipe #echo 'whoop' | grep 'whoop'## ##redirect-stdout #echo 'stdout' > ./output && cat output## ##redirect-stdin #cat < ./input## ##background-task #sleep 1 & echo 'sleeping' && wait## ##stderr #echo 'stderr' >&2## ##stdout-stderr #echo 'stdout' && echo 'stderr' >&2## ##exit-code #exit 123## ##pipe-concat-fail-first #exit 2 | echo 'yeah'## ##pipe-concat-fail-second #echo 'yeah' | exit 2 | echo 'yeah'## ##and-concat-fail #exit 2 && echo 'nope'## ##or-concat-fail #exit 2 || echo 'yeah'## ##long-output #for i in {1..1000}; do echo "$i"; done## ##all-the-quotes #echo "'single' and \"double\""## ##unicode #echo 'ðŸ¤˜'## ##env-fail #echo 'yeah'#{"FAILS":"$(false)"}#
